<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apexmemory.backup</string>

	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>/opt/apexmemory/deploy/backup.sh</string>
	</array>

	<key>StartInterval</key>
	<integer>21600</integer>

	<key>StandardOutPath</key>
	<string>/opt/apexmemory-data/logs/backup-stdout.log</string>

	<key>StandardErrorPath</key>
	<string>/opt/apexmemory-data/logs/backup-stderr.log</string>

	<key>WorkingDirectory</key>
	<string>/opt/apexmemory</string>
</dict>
</plist>
